{
  "name": "koffinate/laravel-menu",
  "description": "Laravel Menu Collector",
  "keywords": [
    "php", "laravel",
    "koffinate", "koffin", "coffee",
    "much", "yusron", "arif", "yusronarif",
    "menu", "laravel menu", "menu collection"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Much Yusron Arif",
      "email": "yusron.arif4@gmail.com",
      "homepage": "https://yusronarif.github.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "illuminate/contracts": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/routing": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Kfn\\Menu\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kfn\\Menu\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "laravel": {
      "providers": [
        "Kfn\\Menu\\MenuServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
